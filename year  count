

// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n1 ;
    
    printf("the year is ");
    scanf("%d", &n1);
    
    if(n1%4==0) {
        printf("It is a leap year");
         }
         
         else {
             printf("it is not a leap year");
         }
    

