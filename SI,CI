#include <stdio.h>
#include <math.h>
int main() {
    // Write C code here
   float n1,n2 ;
   int n3;
   
    printf("enter the principle = ");
    scanf("%f",&n1);
    
    printf(" enter the rate =");
    scanf("%f",&n2);
    
    printf(" enter the time =");
    scanf("%d",&n3);
    
    
   float power= pow((1+n2/100),n3);
    printf("\ncompound interest is =%f",n1*power);

    
}
